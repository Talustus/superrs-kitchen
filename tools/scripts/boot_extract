#!/bin/bash

# Do not edit this file unless you know what you are doing

cd $tools/boot/mkbootimg
if [[ -f $romdir/recovery.img ]]; then
	( exec ./unpackimgr.sh $romdir/recovery.img >> $logs/boot.log )
else
	( exec ./unpackimg.sh $romdir/boot.img >> $logs/boot.log )
fi
cd $scripts
exec ./boot_tools
