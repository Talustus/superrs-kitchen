#!/bin/bash

# Do not edit this file unless you know what you are doing

export chosenimg="recovery"
if [[ ! $(xxd -p $romdir/$chosenimg.img | grep 88168858) = "" ]]; then
	( exec $tools/boot/carliv/scripts/unpack_MTK_img 2>&1 ) >> $logs/boot.log
	touch $romdir/${chosenimg}img/MTK
else			
	( exec $tools/boot/carliv/scripts/unpack_img 2>&1 ) >> $logs/boot.log
fi
exit
