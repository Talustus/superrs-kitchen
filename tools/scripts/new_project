#!/bin/bash

# Do not edit this file unless you know what you are doing

banner() {
	echo ""
	echo "------------------------------------------------------"
	echo "-                                                    -"
	echo "-                 SuperR's Kitchen                   -"
	echo "-                     by SuperR                      -"
	echo "-                                                    -"
	echo "------------------------------------------------------"
	echo ""
}

cd $base

clear
banner
romname=""
romname1=""
echo "Enter new project name (spaces will be replcaed with _):"
read romname1
export romname=$(echo "$romname1" | sed 's/ /_/g')
mkdir superr_$romname
exec ./superr
exit
