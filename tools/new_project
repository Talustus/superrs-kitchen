#!/bin/bash

# Do not edit this file unless you know what you are doing

banner() {
	echo ""
	echo "------------------------------------------------------"
	echo "-                                                    -"
	echo "-                 SuperR's Kitchen                   -"
	echo "-                     by SuperR                      -"
	echo "-                                                    -"
	echo "------------------------------------------------------"
	echo ""
}

cd $base

clear
banner
romname=""
echo "Enter new project name:"
read romname
mkdir superr_$romname
export romname="$romname"
clear
./superr
exit
